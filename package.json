{
  "name": "MysteriumVPN",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "android": "adb reverse tcp:4050 tcp:4050 && react-native run-android",
    "ios": "react-native run-ios",
    "test:unit": "jest",
    "test:e2e:ios": "detox test -o tests/e2e/config.json --configuration=ios.sim.debug",
    "lint": "tslint -p . -c tslint.json 'src/**/*.{ts,tsx}' 'tests/**/*.{ts,tsx}'",
    "ci": "yarn lint && yarn test:unit"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@types/enzyme": "^3.1.11",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.0.41",
    "@types/react-native": "^0.52.19",
    "@types/react-test-renderer": "^16.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "^5.0.2",
    "detox": "^9.0.5",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "23.6.0",
    "jest-html-reporter": "2.4.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-native-mock": "^0.3.1",
    "react-native-scripts": "1.14.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "^16.3.0-alpha.1",
    "ts-jest": "^23.10.4",
    "tslint": "5.11.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "3.6.0",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "computed-async-mobx": "^4.1.0",
    "mobx": "^4.3.1",
    "mobx-react": "^5.1.0",
    "mobx-utils": "^5.0.2",
    "mysterium-tequilapi": "^0.2.2",
    "native-base": "^2.8.1",
    "react": "^16.6.0",
    "react-native": "0.56",
    "react-native-dropdownalert": "^3.7.0",
    "react-native-vector-icons": "^6.0.2"
  },
  "detox": {
    "specs": "e2e",
    "test-runner": "jest",
    "runner-config": "tests/e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/MysteriumVPN.app",
        "build": "xcodebuild -project ios/MysteriumVPN.xcodeproj -scheme MysteriumVPN -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "pushd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && popd",
        "type": "android.emulator",
        "name": "Nexus_5X_API_23"
      }
    }
  }
}
